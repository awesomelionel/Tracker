<%= form_for [@project, @ticket], :html => { :class => "form-stacked"}  do |f| %>
	<%= render 'shared/error_messages' , :object => f.object %>
	<p>
	<%= f.label :title %>
	<%= f.text_field :title %>
	</p>
	<p>
	<%= f.label :description %>
	<%= f.text_area :description %>
	</p>
	<%= f.submit %>
<% end %>
